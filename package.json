{
  "name": "hash-wapm",
  "version": "0.0.1",
  "description": "Hash tools written for AssemblyScript.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:debug": "asc assembly/index.ts -b build/debug.wasm -t build/debug.wat --validate --sourceMap --debug --use abort=wasiabort",
    "asbuild": "asc assembly/index.ts -b build/hash.wasm -t build/hash.wat --validate --optimize --use abort=wasiabort"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shonenada/hash-wapm.git"
  },
  "keywords": [
    "hash",
    "asc",
    "assemblyscript"
  ],
  "author": "shonenada",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/shonenada/hash-wapm/issues"
  },
  "homepage": "https://github.com/shonenada/hash-wapm#readme",
  "devDependencies": {
    "as-wasi": "0.0.1",
    "assemblyscript": "^0.9.2"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.9.2"
  }
}
